class Node:
    def __init__(self, data=None, next=None):
        #  set data and next
        self.data = data
        self.next = next


class LinkedList:
    def __init__(self):
        #initialize head
        self.head = None

    def append(self, data):
        # create new node and attach it at the end
        new_node= Node.data
        if not self.head:
            self.head = new_node
            return
        last = self.head
        while last.next:
            last = last.next
        
        last.next = new_node


    def print_list(self):
        # traverse and print the list
        current = self.head
        # Test your implementation
        while current:
            print(current.data, end =' → ')
            current = current.next
        print("None")


ll = LinkedList()
ll.append(10)
ll.append(20)
ll.append(30)
ll.print_list()  # Expected: 10 → 20 → 30 → None
